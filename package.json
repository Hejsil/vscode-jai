{
    "name": "jai",
    "displayName": "Jai",
    "description": "",
    "version": "0.0.1",
    "publisher": "Hejsil",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "jai",
                "extensions": [
                    ".jai"
                ],
                "aliases": [
                    "Jai"
                ],
                "configuration": "./syntaxes/jai.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jai",
                "scopeName": "source.jai",
                "path": "./syntaxes/jai.json"
            }
        ],
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ],
        "jsonValidation": [
            {
            "fileMatch": "project.json",
            "url": "http://json.schemastore.org/project"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}